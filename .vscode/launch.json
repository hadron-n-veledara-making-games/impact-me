    "version": "0.2.0",
    "configurations": [
    {
        "name": "run telegram worker",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}\\cmd\\telegramworker\\main.go",
        "args": ["-config-path", "${workspaceFolder}\\configs\\telegrambot.toml"]
    },
    {
        "name": "run telegram listener",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}\\cmd\\telegrambot\\main.go",
        "args": ["-config-path", "${workspaceFolder}\\configs\\telegrambot.toml"]
    }

    ]
}